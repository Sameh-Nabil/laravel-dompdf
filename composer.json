{
    "name": "barryvdh/laravel-dompdf",
    "description": "A DOMPDF Wrapper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "dompdf", "pdf", "arabic"],
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "baklysystems/dompdf": "dev-master",
        "illuminate/support": "^6|^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "^4|^5|^6",
        "squizlabs/php_codesniffer": "^3.5",
        "phpro/grumphp": "^1",
        "nunomaduro/larastan": "^0.7.10"
    },
     "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/Sameh-Nabil/dompdf"
      }
    ],
    "autoload": {
        "psr-4": {
            "Barryvdh\\DomPDF\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Barryvdh\\DomPDF\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Barryvdh\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Barryvdh\\DomPDF\\Facade"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=psr12 src/",
        "fix-style": "phpcbf -p --standard=psr12 src/",
        "phpstan": "phpstan analyze --memory-limit=-1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
